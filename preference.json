{
  "basePackage": "to.kit.personal.making",
  "charset": "MS932",
  "generators": [
    {"id":"die", "clazz":"Die"},
    {"id":"custNum", "clazz":"NumberMaker", "args":"100000000, 999999999"},
    {"id":"myNum", "clazz":"MyNumberMaker"},
    {"id":"appType", "clazz":"StringChooser", "args":"new String[]{'1A', '1B'}"},
    {"id":"bizType", "clazz":"StringChooser", "args":"new String[]{'1', '2', '3', '4', '5', '6', '7', '9'}"},
    {"id":"familyName", "clazz":"KanjiKanaMaker", "args":"'/familyName.txt', 'katakana'"},
    {"id":"firstName", "clazz":"KanjiKanaMaker", "args":"'/firstName.txt', 'katakana'"},
    {"id":"companyName", "clazz":"KanjiKanaMaker", "args":"'/companyName.txt', 'katakana'"},
    {"id":"address", "clazz":"AddressMaker", "args":"new String[]{'31', '32', '33', '34', '35'}"},
    {"id":"telephone", "clazz":"TelMaker"},
    {"id":"pcd", "clazz":"StringChooser", "args":"new String[]{'00', '10', '20', '30', '80'}"},
    {"id":"ptp", "clazz":"NumberMaker", "args":"1, 8"},
    {"id":"months", "clazz":"NumberMaker", "args":"6, 360"},
    {"id":"avg", "clazz":"NumberMaker", "args":"300, 1100"}
  ],
  "header": ["A", "B", "C", "D", "E", "F", "G"],
  "outputs": [
    {"var":"appCnt", "value":"appCnt == null ? 0 : appCnt + 1"},
    {"var":"newApp", "value":"appCnt % 4 == 0"},
    {"out":"newApp ? custNum.next() : custNum.current()"},
    {"out":"'00'"},
    {"var":"myNumber", "value":"myNum.next()"},
    {"out":"myNumber.substring(0, 4)"}, {"out":"myNumber.substring(4)"},
    {"out":"1"},
    {"out":"'00'"},
    {"out":"5"},
    {"out":"appType.next()"},
    {"out":"bizType.next()"},
    {"var":"sei", "value":"newApp ? familyName.next() : familyName.current()"},
    {"var":"mei", "value":"newApp ? firstName.next() : firstName.current()"},
    {"var":"newName", "value":"die.next(20) == 0"},
    {"var":"appSei", "value":"newName && die.next(20) == 0 ? familyName.next() : familyName.current()"},
    {"var":"appMei", "value":"newName ? firstName.next() : firstName.current()"},
    {"out":"appSei.kanji + '\u3000' + appMei.kanji"},
    {"out":"appSei.kana + '\u3000' + appMei.kana"},
    {"var":"addr", "value":"newApp ? address.next() : address.current()"},
    {"var":"tel", "value":"telephone.next(addr.zip7).split('-')"},
    {"out":"tel[0]"},
    {"out":"tel[1]"},
    {"out":"tel[2]"},
    {"out":"addr.zip7"},
    {"out":"''"},
    {"out":"''"},
    {"out":"addr.pref + addr.munic"},
    {"out":"addr.city"},
    {"out":"addr.street"},
    {"out":"addr.apartment"},
    {"out":"addr.room"},
    {"out":"''"},
    {"var":"com", "value":"companyName.next()"},
    {"var":"hasCom", "value":"die.next(100) == 0"},
    {"out":"hasCom ? com.kanji : ''"},
    {"out":"hasCom ? com.kana : ''"},
    {"out":"sei.kanji + '\u3000' + mei.kanji"},
    {"out":"sei.kana + '\u3000' + mei.kana"},
    {"out":"addr.zip7"},
    {"out":"''"},
    {"out":"''"},
    {"out":"addr.pref + addr.munic"},
    {"out":"addr.city"},
    {"out":"addr.street"},
    {"out":"addr.apartment"},
    {"out":"addr.room"},
    {"out":"''"},
    {"out":"pcd.next()"},
    {"out":"ptp.next()"},
    {"out":"1"},
    {"out":"months.next()"},
    {"var":"format", "value":"new java.text.DecimalFormat('#.00')"},
    {"var":"avguse", "value":"avg.next() * 1.13"},
    {"var":"amount", "value":"avguse * 13"},
    {"var":"c1", "value":"amount * 0.93"},
    {"var":"c2", "value":"amount * 0.96"},
    {"out":"format.format(avguse)"},
    {"out":"format.format(amount)"},
    {"out":"format.format(c1)"},
    {"out":"format.format(c2)"},
  ],
  "requests": 10
}
